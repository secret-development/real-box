<%= flash_msg(flash[:notice]) %>
<%= form_tag general_reports_path, :method => 'get' do %>
  <%= label_tag :period, "Period" %>
  <%= select_tag :period, options_for_select(ApplicationHelper::MONTH) %>
  <%= select_tag :objects, options_for_select(ApplicationHelper::OBJECTS) %>
  <%= submit_tag 'Build chart', :class => "btn" %>
<% end %>
